module dig2to1 (
    input num[8],
    output out[2][4]
  ) {
  
  always {
    if(num < 10){
      out = {4d0, num[3:0]};
    } else {
      case(num){
        10: out = {4d1,4d0};
        11: out = {4d1,4d1};
        12: out = {4d1,4d2};
        13: out = {4d1,4d3};
        14: out = {4d1,4d4};
        15: out = {4d1,4d5};
        16: out = {4d1, 4d6};
        default: out = {4d0, 4d0};
      }
    }
  }
}